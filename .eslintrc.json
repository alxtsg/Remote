{
  "extends": "airbnb",
  "plugins": [
    "import"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": "off",
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "object-shorthand": ["error", "consistent"],
    "strict": ["error", "global"]
  }
}
